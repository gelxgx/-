.scroll-view-item{
    display:flex;
    justify-content:center;
    padding-top:20rpx; 
}
.scroll-view-item:last-child{
    padding-bottom:20rpx;
}
.scroll-view-item image{
    width:90rpx;
    height:90rpx;
    opacity: 1;
}
.scroll-view-item image.filter{
    /* -webkit-filter: grayscale(100%);  */
    /* filter: grayscale(100%);*/
    opacity: .6;
}
.search-input.hide-search{
    width: 0rpx;
    opacity:0;
}
.search-input.show-search{
    width:453rpx;
    opacity:1;
}
.search-input{
    background:#fff;
    width:453rpx;
    height:70rpx;
    margin-left:22rpx;
    border-radius:30rpx;
    font-size:30rpx;
    /* padding-left:20rpx; */
    box-sizing: border-box;
    transition:all 1s;
    align-items:center;
    display:flex;
}
.search-input input {
    padding-left:35rpx;
}

.scope-wrapper{
   padding:20rpx 10rpx;
   height:176rpx;
   display:flex;
   box-sizing:border-box;
   align-items:center;
   background: url('//yitian.happyhao.top/assets/location_bg.png') no-repeat;
   background-size:cover;
}
.scope-wrapper-handle{
    display:flex;
    justify-content:center;
    width:100%;
}

.scope-wrapper-handle button{
    border-radius:10rpx;
    background: #A0948E;
    color:#fff;
    width:403rpx;
}
.wc {
    width:90rpx;
    height:90rpx;
    opacity: 1;
    position: relative;
    top: 80%;
}
