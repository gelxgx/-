   position: fixed;
   top:109rpx;
   left:26rpx;
   z-index:999;
   display:flex;
   align-items:center;
}
.search-box image{
    width:86rpx;
    height:88rpx;
}
.top-menu{
    position: fixed;
    top:109rpx;
    right:26rpx;
    z-index:999;
    display:flex;
    flex-direction: column;
}
.location-box{
    position: fixed;
    left:26rpx;
    bottom:192rpx;
    z-index:999;
}
.go-index-page{
    position:fixed;
    top: 260rpx;
    right: 24rpx;
    z-index: 999;
}
.go-index-page image{
   width:104rpx;
   height:104rpx;
}
.go-detail-page{
    position:fixed;
    top: 132rpx;
    right: 24rpx;
    z-index: 999;
}
.go-detail-page image{
   width:104rpx;
   height:104rpx;
}
.location-box image{
    width:106rpx;
